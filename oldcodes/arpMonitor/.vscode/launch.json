{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Electron: Main",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "runtimeArgs": [
        "."
      ],
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/electron/dist/electron.exe"
      },
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "ELECTRON_DISABLE_SECURITY_WARNINGS": "true"
      }
    }
  ]
}
