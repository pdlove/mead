    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\server.js",
            "runtimeArgs": ["--preserve-symlinks"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test Syslog Job",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\stacks\\syslog\\jobs\\syslogCollector.js",
            "runtimeArgs": ["--preserve-symlinks"]
        }
    ]
}